response = input("Welcome to the binary to ASCII (text) or ASCII to binary converter. Please select \"Binary\" for binary > ASCII, or \"ASCII\" for ASCII > binary.")
print("1. Binary > ASCII")
print("2. ASCII > Binary ")

response == 'Binary':

    binary = input("")

    bytes = binary.split()

    chrs = [chr(int(byte, 2)) for byte in bytes]

    print("".join(chrs))

